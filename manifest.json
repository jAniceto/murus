{
    "manifest_version": 2,
    "name": "Murus",
    "version": "0.1.0",
    "description": "Launches apps, the good way!",
    "permissions" : ["tabs", "storage"],
    "chrome_url_overrides" : {
        "newtab": "index.html"
    },
    "background": {
        "scripts": ["js/background.js"],
        "persistent": false
    },
    "options_page": "options.html",
    "icons": { 
        "16": "img/icon16.png",
        "48": "img/icon48.png",
       "128": "img/icon128.png" 
    }
}